
image{
  width: 100%
}
.classify{
  display: flex;
}
.classify-item{
  flex: 1;
  display: flex;
  flex-direction:column;
  align-items: center;
  font-size: 13px;
  margin-top: 8px;
}
.classify-item image{
  width: 140rpx;
  height: 140rpx;
}
.floor{
  margin: 10px 0 0 5px;
}
.floor>image{
  height: 60rpx
}
.floor-box{
  /* margin-top: 50px */
}
.floor-box navigator:nth-child(1){
  width: 30%;
  float: left;
  margin-right: 20rpx
}
.floor-box navigator:nth-child(1) image{
  height: 386rpx
}
.floor-box navigator:nth-child(2),
.floor-box navigator:nth-child(3),
.floor-box navigator:nth-child(4),
.floor-box navigator:nth-child(5)
{
  float: left;
  width: 30%;
  margin-right: 20rpx;
  margin-bottom: 2rpx
}
.floor-box navigator:nth-child(2) image,
.floor-box navigator:nth-child(3) image,
.floor-box navigator:nth-child(4) image,
.floor-box navigator:nth-child(5) image{
  height: 188rpx;
}